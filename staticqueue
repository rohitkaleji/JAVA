Q3 Write a program to implement Static Queue

#include <stdio.h>
#define MAX_SIZE 10

struct StaticQueue {

 int array[MAX_SIZE];

 int front, rear;

};

void initialize(struct StaticQueue* queue) {

 queue->front = -1;

 queue->rear = -1;

}

int isEmpty(struct StaticQueue* queue) {

 return queue->front == -1;

}

int isFull(struct StaticQueue* queue) {

 return (queue->rear + 1) % MAX_SIZE == queue->front;

}

void enqueue(struct StaticQueue* queue, int value) {

 if (isFull(queue)) {

 printf("Queue Overflow. Cannot enqueue element %d\n", value);

 } else {

 if (isEmpty(queue)) {

 queue->front = 0;

 }

 queue->rear = (queue->rear + 1) % MAX_SIZE;
queue->array[queue->rear] = value;

 printf("Enqueued %d\n", value);

 }

}

int dequeue(struct StaticQueue* queue) {

 if (isEmpty(queue)) {

 printf("Queue Underflow. Cannot dequeue from an empty 

queue\n");

 return -1; // Return a sentinel value to indicate an error

 } else {

 int dequeuedValue = queue->array[queue->front];

 if (queue->front == queue->rear) {

 initialize(queue);

 } else {

 queue->front = (queue->front + 1) % MAX_SIZE;

 }

 printf("Dequeued %d\n", dequeuedValue);

 return dequeuedValue;

 }

}

void display(struct StaticQueue* queue) {

 if (isEmpty(queue)) {

 printf("Queue is empty\n");

 } else {

 printf("Queue elements: ");

 int i = queue->front;
do {

 printf("%d ", queue->array[i]);

 i = (i + 1) % MAX_SIZE;

 } while (i != (queue->rear + 1) % MAX_SIZE);

 printf("\n");

 }

}

int main() {

 struct StaticQueue queue;

 initialize(&queue);

 enqueue(&queue, 5);

 enqueue(&queue, 10);

 enqueue(&queue, 15);

 display(&queue);

 dequeue(&queue);

 display(&queue);

 return 0;

}

OP:

Enqueued 5

Enqueued 10

Enqueued 15

Queue elements: 5 10 15
Dequeued 5

Queue elements: 10 1
