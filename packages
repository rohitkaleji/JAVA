package letmecalculate;

public class Cal {
    public int sub(int a,int b){
	return a-b;
	}
public int div (int a,int b){
	return a/b;
}
public int mod (int a,int b){
	return a%b;
}
}






package letmecalculate;

public class calculator {
    public int add(int a, int b) {
        return a + b;
    }

    public int mul(int a, int b) {
        return a * b;
    }
}








import letmecalculate.*;
public class Demo{
   public static void main(String args[]){
     calculator obj=new calculator();
	 Cal obj1=new Cal();
	 System.out.println(obj.add(40,900));
     System.out.println(obj.mul(60,200));
	 System.out.println(obj1.sub(100,50));
	 System.out.println(obj1.div(200,2));
	 System.out.println(obj1.mod(270,2));
	}
}
